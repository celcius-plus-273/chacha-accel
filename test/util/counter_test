#! /opt/homebrew/Cellar/icarus-verilog/12.0/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/system.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/vhdl_sys.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/vhdl_textio.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/v2005_math.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/va_math.vpi";
S_0x13cf058f0 .scope module, "tb" "tb" 2 1;
 .timescale 0 0;
P_0x600002449800 .param/l "CLOCK_PERIOD" 1 2 4, +C4<00000000000000000000000000001010>;
v0x60000034c000_0 .var "clock", 0 0;
v0x60000034c090_0 .var "counter", 4 0;
v0x60000034c120_0 .var/i "cycles", 31 0;
v0x60000034c1b0_0 .var "state", 0 0;
E_0x600002449880 .event posedge, v0x60000034c000_0;
    .scope S_0x13cf058f0;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x60000034c000_0, 0, 1;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x60000034c120_0, 0, 32;
    %pushi/vec4 0, 0, 5;
    %store/vec4 v0x60000034c090_0, 0, 5;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x60000034c1b0_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x13cf058f0;
T_1 ;
    %load/vec4 v0x60000034c000_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %load/vec4 v0x60000034c000_0;
    %inv;
    %vpi_call 2 8 "$display", "Falling Edge: %0b -> %0b", v0x60000034c000_0, S<0,vec4,u1> {1 0 0};
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x60000034c000_0;
    %inv;
    %vpi_call 2 10 "$display", "Rising Edge: %0b -> %0b", v0x60000034c000_0, S<0,vec4,u1> {1 0 0};
T_1.1 ;
    %delay 5, 0;
    %load/vec4 v0x60000034c000_0;
    %inv;
    %store/vec4 v0x60000034c000_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x13cf058f0;
T_2 ;
    %delay 10, 0;
    %load/vec4 v0x60000034c120_0;
    %addi 1, 0, 32;
    %store/vec4 v0x60000034c120_0, 0, 32;
    %vpi_call 2 20 "$display", "Cycle count: %0d", v0x60000034c120_0 {0 0 0};
    %jmp T_2;
    .thread T_2;
    .scope S_0x13cf058f0;
T_3 ;
    %pushi/vec4 0, 0, 5;
    %assign/vec4 v0x60000034c090_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x60000034c1b0_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x60000034c000_0, 0;
    %delay 100, 0;
    %vpi_call 2 31 "$finish" {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x13cf058f0;
T_4 ;
    %wait E_0x600002449880;
    %load/vec4 v0x60000034c1b0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_4.0, 8;
    %load/vec4 v0x60000034c090_0;
    %pad/u 32;
    %addi 1, 0, 32;
    %vpi_call 2 36 "$display", "counter: %0d -> %0d", v0x60000034c090_0, S<0,vec4,u32> {1 0 0};
    %load/vec4 v0x60000034c090_0;
    %addi 1, 0, 5;
    %assign/vec4 v0x60000034c090_0, 0;
    %load/vec4 v0x60000034c090_0;
    %pad/u 32;
    %cmpi/e 5, 0, 32;
    %jmp/0xz  T_4.2, 4;
    %vpi_call 2 40 "$display", "I also reached 5!" {0 0 0};
T_4.2 ;
T_4.0 ;
    %load/vec4 v0x60000034c090_0;
    %pad/u 32;
    %cmpi/e 5, 0, 32;
    %jmp/0xz  T_4.4, 4;
    %vpi_call 2 44 "$display", "counter reach 5!" {0 0 0};
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x60000034c1b0_0, 0;
    %pushi/vec4 0, 0, 5;
    %assign/vec4 v0x60000034c090_0, 0;
T_4.4 ;
    %jmp T_4;
    .thread T_4;
